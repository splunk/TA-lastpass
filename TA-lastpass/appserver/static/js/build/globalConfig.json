{
    "meta": {
        "version": "1.0.0", 
        "restRoot": "TA_lastpass", 
        "name": "TA-lastpass", 
        "displayName": "LastPass", 
        "apiVersion": "3.0.0"
    }, 
    "pages": {
        "inputs": {
            "table": {
                "header": [
                    {
                        "field": "name", 
                        "label": "Name"
                    }, 
                    {
                        "field": "interval", 
                        "label": "Interval"
                    }, 
                    {
                        "field": "index", 
                        "label": "Index"
                    }, 
                    {
                        "field": "disabled", 
                        "label": "Status"
                    }
                ], 
                "moreInfo": [
                    {
                        "field": "name", 
                        "label": "Name"
                    }, 
                    {
                        "field": "interval", 
                        "label": "Interval"
                    }, 
                    {
                        "field": "index", 
                        "label": "Index"
                    }, 
                    {
                        "field": "disabled", 
                        "label": "Status"
                    }, 
                    {
                        "field": "lastpass_api_url", 
                        "label": "LastPass API URL"
                    }, 
                    {
                        "field": "time_start", 
                        "label": "Collection Start Time"
                    }
                ], 
                "actions": [
                    "edit", 
                    "enable", 
                    "delete", 
                    "clone"
                ]
            }, 
            "services": [
                {
                    "name": "lastpass_reporting", 
                    "entity": [
                        {
                            "required": true, 
                            "type": "text", 
                            "label": "Name", 
                            "field": "name", 
                            "help": "Enter a unique name for the data input", 
                            "validators": [
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "type": "regex"
                                }, 
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "type": "string", 
                                    "maxLength": 100
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "type": "text", 
                            "label": "Interval", 
                            "field": "interval", 
                            "help": "Time interval of input in seconds.", 
                            "validators": [
                                {
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$", 
                                    "errorMsg": "Interval must be an integer.", 
                                    "type": "regex"
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "defaultValue": "default", 
                            "options": {
                                "blackList": "^_.*$", 
                                "createSearchChoice": true, 
                                "endpointUrl": "data/indexes"
                            }, 
                            "type": "singleSelect", 
                            "label": "Index", 
                            "field": "index", 
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "type": "string", 
                                    "maxLength": 80
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "options": {
                                "placeholder": "https://lastpass.com/enterpriseapi.php"
                            }, 
                            "type": "text", 
                            "label": "LastPass API URL", 
                            "field": "lastpass_api_url", 
                            "help": "Lastpass reporting REST API endpoint URL", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }, 
                        {
                            "required": false, 
                            "options": {
                                "placeholder": "timestamp for start of event collection"
                            }, 
                            "type": "text", 
                            "label": "Collection Start Time", 
                            "field": "time_start", 
                            "help": "Supports two formats: epoch and YYYY-mm-dd HH:MM:SS. Supported timezone: PST.\n If there exists a time checkpoint, this value may be ignored.", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }
                    ], 
                    "title": "LastPass Reporting"
                }
            ], 
            "description": "Manage your data inputs", 
            "title": "Inputs"
        }, 
        "configuration": {
            "tabs": [
                {
                    "name": "logging", 
                    "entity": [
                        {
                            "field": "loglevel", 
                            "defaultValue": "INFO", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "DEBUG", 
                                        "value": "DEBUG"
                                    }, 
                                    {
                                        "label": "INFO", 
                                        "value": "INFO"
                                    }, 
                                    {
                                        "label": "WARNING", 
                                        "value": "WARNING"
                                    }, 
                                    {
                                        "label": "ERROR", 
                                        "value": "ERROR"
                                    }, 
                                    {
                                        "label": "CRITICAL", 
                                        "value": "CRITICAL"
                                    }
                                ], 
                                "disableSearch": true
                            }, 
                            "type": "singleSelect", 
                            "label": "Log level"
                        }
                    ], 
                    "title": "Logging"
                }, 
                {
                    "name": "additional_parameters", 
                    "entity": [
                        {
                            "required": true, 
                            "defaultValue": "", 
                            "type": "text", 
                            "label": "LastPass Enterprise id", 
                            "field": "cid", 
                            "help": "Customer id", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }, 
                        {
                            "encrypted": true, 
                            "required": true, 
                            "defaultValue": "", 
                            "type": "text", 
                            "label": "LastPass Provisioning Hash", 
                            "field": "provhash", 
                            "help": "Used for API access", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of password is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }
                    ], 
                    "title": "Add-on Settings"
                }
            ], 
            "description": "Set up your add-on", 
            "title": "Configuration"
        }
    }
}